NAME=linkchannelstat
SIMULATOR=ns2
RADIO=shadowing11b
FORGEINNODES=no

TIME=450

NODEPLACEMENTFILE=placement.default
NODETABLE=linkstat.mes
NODEPLACEMENT=grid
NODEPLACEMENTOPTS="relative"
FIELDSIZE=RXRANGE

LOGDIR=WORKDIR
LOGFILE=measurement.log

RESULTDIR=WORKDIR
EVALUATION="embrace_xml linkstat_graph network_info"
EVALUATIONSDIR=WORKDIR/evaluation

GPS=no
NOTICE=no
LOS=no

CONTROLSOCKET=yes

MULTIREPEAT="1"
MULTIWAIT=0
MULTIRUNMODE=REBOOT

PARAMSFILE="params"
PARAMSWAIT=0
PARAMSRUNMODE=REBOOT
