NAME=linkchannelstat
SIMULATOR=ns2
RADIO=shadowing
FORGEINNODES=no

TIME=350

NODEPLACEMENTFILE=placement.default
NODETABLE=linkstat.mes
NODEPLACEMENT=grid
NODEPLACEMENTOPTS="relative"
FIELDSIZE=RXRANGE

LOGDIR=WORKDIR
LOGFILE=measurement.log

RESULTDIR=WORKDIR
#EVALUATION="embrace_xml network_info_viz"
EVALUATION="evaluation/copy_tmp.sh embrace_xml status_compression evaluation/eval.sh network_info_viz"
EVALUATIONSDIR=WORKDIR/evaluation

GPS=no
NOTICE=no
LOS=no

#LOCALPROCESS=CONFIGDIR/linkstat_ctl.sh

CONTROLSOCKET=yes

MULTIREPEAT="1"
MULTIWAIT=0
MULTIRUNMODE=REBOOT

PARAMSFILE="params"
PARAMSWAIT=0
PARAMSRUNMODE=REBOOT
