NAME=linkchannelstat_plm
SIMULATOR=ns2
RADIO=shadowing11b
FORGEINNODES=no

TIME=350

NODEPLACEMENTFILE=placement.default
NODETABLE=linkstat_plm.mes
NODEPLACEMENT=npart
NODEPLACEMENTOPTS="relative"
FIELDSIZE=RXRANGE

LOGDIR=WORKDIR
LOGFILE=measurement.log

RESULTDIR=WORKDIR
#EVALUATION="embrace_xml linkstat_graph network_info"
EVALUATION="evaluation/copy_tmp.sh embrace_xml status_compression evaluation/eval.sh linkstat_graph network_info"
#EVALUATION="evaluation/copy_tmp.sh embrace_xml evaluation/eval.sh linkstat_graph"
EVALUATIONSDIR=WORKDIR/evaluation

GPS=no
NOTICE=no
LOS=no

#LOCALPROCESS=CONFIGDIR/linkstat_ctl.sh

CONTROLSOCKET=yes

MULTIREPEAT="1"
MULTIWAIT=0
MULTIRUNMODE=REBOOT

PARAMSFILE="params"
PARAMSWAIT=0
PARAMSRUNMODE=REBOOT
